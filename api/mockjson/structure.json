{
  "thirdParty": "Lumary",
  "fields": [
    {
      "label": "Delivery Methods",
      "key": "slServiceDeliveryMethods",
      "type": "select",
      "apiKey": "skedhealthcare__Delivery_Method__c",
      "validate": {
        "required": true
      },
      "dataSource": {
        "type": "onLoad",
        "dataKey": "serviceDeliveryMethods"
      },
      "defaultDisplay": true,
      "persit": true
    },
    {
      "label": "Category",
      "key": "slCategory",
      "type": "select",
      "apiKey": "category",
      "ref": [
        "skedhealthcare__Service__c",
        "skedhealthcare__Service_Agreement_Item__c"
      ],
      "validate": {
        "required": true
      },
      "dataSource": {
        "type": "onLoad",
        "dataKey": "categories"
      },
      "defaultDisplay": true
    },
    {
      "label": "Service",
      "key": "slService",
      "type": "select",
      "apiKey": "skedhealthcare__Service__c",
      "validate": {
        "required": true
      },
      "ref": [
        "skedhealthcare__Site__c"
      ],
      "refAction": {
        "operate": "AND",
        "show": [
          {
            "apiKey": "category",
            "condition": "!=",
            "value": "Uncategorised"
          },
          {
            "apiKey": "category",
            "condition": "!=",
            "value": null
          }
        ],
        "getData": true
      },
      "dataSource": {
        "type": "api",
        "apiName": "getServices",
        "paramKey": [
          {
            "apiKey": "category",
            "path": "value"
          }
        ],
        "param": "category : {0}"
      },
      "defaultDisplay": false,
      "persit": true
    },
    {
      "label": "Site",
      "key": "slSite",
      "type": "select",
      "apiKey": "skedhealthcare__Site__c",
      "validate": {
        "required": true
      },
      "ref": [
        "skedhealthcare__Service_Agreement_Item__c"
      ],
      "refAction": {
        "operate": "AND",
        "show": [
          {
            "apiKey": "skedhealthcare__Service__c",
            "condition": "!=",
            "value": null
          }
        ],
        "getData": true
      },
      "dataSource": {
        "type": "api",
        "apiName": "getSites",
        "paramKey": [
          {
            "apiKey": "skedhealthcare__Service__c",
            "path": "value"
          }
        ],
        "param": "service : {0}"
      },
      "defaultDisplay": false,
      "persit": true
    },
    {
      "label": "Service Agreement Item",
      "key": "slServiceAgreementItem",
      "type": "select",
      "apiKey": "skedhealthcare__Service_Agreement_Item__c",
      "validate": {
        "required": true
      },
      "ref": [
        "skedhealthcare__Rate__c"
      ],
      "refAction": {
        "operate": "OR",
        "show": [
          {
            "apiKey": "skedhealthcare__Site__c",
            "condition": "!=",
            "value": null
          },
          {
            "apiKey": "category",
            "condition": "==",
            "value": "Uncategorised"
          }
        ],
        "getData": true
      },
      "dataSource": {
        "type": "api",
        "apiName": "getServiceAgreementItems",
        "paramKey": [
          {
            "apiKey": "skedhealthcare__Service__c",
            "path": "value"
          }
        ],
        "param": "service : {0}"
      },
      "defaultDisplay": false,
      "persit": true
    },
    {
      "label": "Rate",
      "key": "slRate",
      "type": "select",
      "apiKey": "skedhealthcare__Rate__c",
      "validate": {
        "required": true
      },
      "refAction": {
        "operate": "AND",
        "show": [
          {
            "apiKey": "skedhealthcare__Service_Agreement_Item__c",
            "condition": "!=",
            "value": null
          },
          {
            "apiKey": "skedhealthcare__Service__c",
            "condition": "!=",
            "value": null
          }
        ],
        "getData": true
      },
      "dataSource": {
        "type": "api",
        "apiName": "getRates",
        "paramKey": [
          {
            "apiKey": "skedhealthcare__Rate__c",
            "path": "value"
          }
        ],
        "param": "service : {0}"
      },
      "defaultDisplay": false,
      "persit": true
    },
    {
      "label": "Save",
      "key": "btnSave",
      "type": "button",
      "buttonType": "primary",
      "apiKey": "btnSave",
